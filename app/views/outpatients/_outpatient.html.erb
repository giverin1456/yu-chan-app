<%= form_with(model:[@user,@outpatient], local: true) do |form| %>
  <div class="price">
    <%= form.label :price %>
    <%= form.text_field :price %>
  </div>
  <div class="text">
    <%= form.label :text%>
    <%= form.text_field :text %>
  </div>
  <div class="start_time">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>
  <div class="submit">
    <%= form.submit %>
  </div>
<% end %>